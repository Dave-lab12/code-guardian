{
    "containerDefinitions": [
        {
            "cpu": 0,
            "dependsOn": [
                {
                    "condition": "START",
                    "containerName": "chroma"
                }
            ],
            "environment": [
                {
                    "name": "CHROMA_URL",
                    "value": "http://chroma:8000"
                },
                {
                    "name": "NODE_ENV",
                    "value": "production"
                }
            ],
            "environmentFiles": [],
            "essential": true,
            "image": "521520215337.dkr.ecr.us-east-1.amazonaws.com/staging-code-guardian",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/staging-code-guardian/starscream",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "mountPoints": [],
            "name": "starscream",
            "portMappings": [
                {
                    "appProtocol": "http",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "name": "starscream-3000-tcp",
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "ulimits": [],
            "volumesFrom": []
        },
        {
            "cpu": 0,
            "environment": [
                {
                    "name": "ANONYMIZED_TELEMETRY",
                    "value": "FALSE"
                },
                {
                    "name": "CHROMA_CORS_ALLOW_ORIGINS",
                    "value": "[\"*\"]"
                },
                {
                    "name": "IS_PERSISTENT",
                    "value": "TRUE"
                },
                {
                    "name": "PERSIST_DIRECTORY",
                    "value": "/chroma/chroma"
                },
                {
                    "name": "CHROMA_SERVER_CORS_ALLOW_ORIGINS",
                    "value": "[\"*\"]"
                }
            ],
            "environmentFiles": [],
            "essential": true,
            "image": "chromadb/chroma:latest",
            "mountPoints": [],
            "name": "chroma",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "name": "chroma-8000-http",
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "cpu": "512",
    "enableFaultInjection": false,
    "executionRoleArn": "arn:aws:iam::521520215337:role/ecsTaskExecutionRole",
    "family": "staging-code-guardian",
    "memory": "1024",
    "networkMode": "awsvpc",
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "volumes": [],
    "tags": []
}